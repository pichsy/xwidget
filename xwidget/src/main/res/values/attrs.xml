    <attr name="xp_pressedAlpha" format="float" />
    <attr name="xp_disabledAlpha" format="float" />
    <!--  控件按压透明度变化效果 end  -->
    <!--  控件按压按钮大小变化效果 start  -->
    <attr name="xp_pressedScale" format="float" />
    <attr name="xp_disabledScale" format="float" />
