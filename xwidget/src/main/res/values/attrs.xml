        <enum name="src" value="0" />
        <enum name="src_atop" value="1" />
        <enum name="src_in" value="2" />
        <enum name="src_out" value="3" />
        <enum name="src_over" value="4" />
        <enum name="multiply" value="5" />
        <enum name="dst" value="6" />
        <enum name="dst_atop" value="7" />
        <enum name="dst_in" value="8" />
        <enum name="dst_out" value="9" />
        <enum name="dst_over" value="10" />
        <enum name="clear" value="11" />
        <enum name="xor" value="12" />
        <enum name="screen" value="13" />
        <enum name="darken" value="14" />
        <enum name="lighten" value="15" />
        <enum name="add" value="16" />
        <enum name="overly" value="17" />
    </attr>
