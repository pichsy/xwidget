    <attr name="xp_leftDividerInsetBottom" format="dimension" />
    <attr name="xp_rightDividerWidth" format="dimension" />
    <attr name="xp_rightDividerColor" format="color|reference" />
    <attr name="xp_rightDividerInsetTop" format="dimension" />
    <attr name="xp_rightDividerInsetBottom" format="dimension" />
    <!--  shadow  -->
    <attr name="xp_showBorderOnlyBeforeL" format="boolean" />
    <attr name="xp_outerNormalColor" format="color|reference" />
    <attr name="xp_outlineInsetTop" format="dimension" />
    <attr name="xp_outlineInsetLeft" format="dimension" />
    <attr name="xp_outlineInsetRight" format="dimension" />
    <attr name="xp_outlineInsetBottom" format="dimension" />
    <attr name="xp_outlineExcludePadding" format="boolean" />

    <!--  给背景设置渐变色  -->
    <attr name="xp_backgroundGradientColors" format="string" />
    <attr name="xp_backgroundGradientStartColor" format="color|reference" />
    <attr name="xp_backgroundGradientEndColor" format="color|reference" />
    <attr name="xp_backgroundGradientOrientation" format="enum">
        <!--    从左到右    -->
        <enum name="horizontal" value="0" />
        <!--    从上到下    -->
        <enum name="vertical" value="1" />
        <!--   左上到右下     -->
        <enum name="TL_BR" value="2" />
        <!--    左下到右上    -->
        <enum name="BL_TR" value="3" />
    </attr>

    <!--  给边框设置渐变色  -->
    <attr name="xp_borderGradientStartColor" format="color|reference" />
    <attr name="xp_borderGradientEndColor" format="color|reference" />
    <!--  渐变边框方向  -->
    <attr name="xp_borderGradientOrientation" format="enum">
        <!--    从左到右    -->
        <enum name="horizontal" value="0" />
        <!--    从上到下    -->
        <enum name="vertical" value="1" />
        <!--   左上到右下     -->
        <enum name="TL_BR" value="2" />
        <!--    左下到右上    -->
        <enum name="BL_TR" value="3" />
    </attr>

    <!--  多状态，背景色效果 start  -->

    <attr name="xp_pressedBackgroundGradientColors" format="string" />
    <attr name="xp_pressedBackground" format="color|reference" />
    <attr name="xp_pressedBackgroundStartColor" format="color|reference" />
    <attr name="xp_pressedBackgroundEndColor" format="color|reference" />
