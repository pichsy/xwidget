<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--  常用属性  -->
    <attr name="xp_radius" format="dimension" />
    <!--  是否使用自动识别边界最小边半圆弧,优先级高于radius  -->
    <attr name="xp_isRadiusAdjustBounds" format="boolean" />
    <attr name="xp_borderColor" format="color|reference" />
    <attr name="xp_borderWidth" format="dimension" />
    <attr name="xp_shadowElevation" format="dimension" />
    <attr name="xp_shadowAlpha" format="float" />
    <attr name="xp_shadowColor" format="color|reference" />
    <attr name="xp_hideRadiusSide" format="enum">
        <enum name="none" value="0" />
        <enum name="top" value="1" />
        <enum name="right" value="2" />
        <enum name="bottom" value="3" />
        <enum name="left" value="4" />
    </attr>
    <!--  layout  -->
    <attr name="xp_bottomDividerHeight" format="dimension" />
    <attr name="xp_bottomDividerColor" format="color|reference" />
    <attr name="xp_bottomDividerInsetLeft" format="dimension" />
    <attr name="xp_bottomDividerInsetRight" format="dimension" />
    <attr name="xp_topDividerHeight" format="dimension" />
    <attr name="xp_topDividerColor" format="color|reference" />
    <attr name="xp_topDividerInsetLeft" format="dimension" />
    <attr name="xp_topDividerInsetRight" format="dimension" />
    <attr name="xp_leftDividerWidth" format="dimension" />
    <attr name="xp_leftDividerColor" format="color|reference" />
